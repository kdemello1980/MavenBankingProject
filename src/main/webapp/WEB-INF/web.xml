<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 <display-name>MavenBankingProject</display-name>
 <servlet>
  <servlet-name>AccountServlet</servlet-name>
  <servlet-class>com.revature.mavenbanking.servlet.AccountServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>PassTimeServlet</servlet-name>
  <servlet-class>com.revature.mavenbanking.servlet.PassTimeServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>LoginServlet</servlet-name>
  <servlet-class>com.revature.mavenbanking.servlet.LoginServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>UserServlet</servlet-name>
  <servlet-class>com.revature.mavenbanking.servlet.UserServlet</servlet-class>
 </servlet>
 <servlet>
  <display-name>AccountDetailServlet</display-name>
  <servlet-name>AccountDetailServlet</servlet-name>
  <servlet-class>com.revature.mavenbanking.servlet.AccountDetailServlet</servlet-class>
 </servlet>
 <servlet>
  <display-name>AddAccountServlet</display-name>
  <servlet-name>AddAccountServlet</servlet-name>
  <servlet-class>com.revature.mavenbanking.servlet.AddAccountServlet</servlet-class>
 </servlet>
 <servlet>
  <display-name>AccountDepositServlet</display-name>
  <servlet-name>AccountDepositServlet</servlet-name>
  <servlet-class>com.revature.mavenbanking.servlet.AccountDepositServlet</servlet-class>
 </servlet>
 <servlet>
  <display-name>AccountWithdrawServlet</display-name>
  <servlet-name>AccountWithdrawServlet</servlet-name>
  <servlet-class>com.revature.mavenbanking.servlet.AccountWithdrawServlet</servlet-class>
 </servlet>
 <servlet>
  <display-name>AccountTransferServlet</display-name>
  <servlet-name>AccountTransferServlet</servlet-name>
  <servlet-class>com.revature.mavenbanking.servlet.AccountTransferServlet</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>AccountServlet</servlet-name>
  <url-pattern>/accounts/*</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>AccountServlet</servlet-name>
  <url-pattern>/accounts/status/*</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>AccountServlet</servlet-name>
  <url-pattern>/accounts/owner/*</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>AccountServlet</servlet-name>
  <url-pattern>/withdraw</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>AccountServlet</servlet-name>
  <url-pattern>/deposit</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>AccountServlet</servlet-name>
  <url-pattern>/transfer</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>PassTimeServlet</servlet-name>
  <url-pattern>/passTime</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>LoginServlet</servlet-name>
  <url-pattern>/login</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>UserServlet</servlet-name>
  <url-pattern>/users/*</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>AccountDetailServlet</servlet-name>
  <url-pattern>/AccountDetailServlet</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>AddAccountServlet</servlet-name>
  <url-pattern>/AddAccountServlet</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>AccountDepositServlet</servlet-name>
  <url-pattern>/AccountDepositServlet</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>AccountWithdrawServlet</servlet-name>
  <url-pattern>/AccountWithdrawServlet</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>AccountTransferServlet</servlet-name>
  <url-pattern>/AccountTransferServlet</url-pattern>
 </servlet-mapping>
 <welcome-file-list>
  <welcome-file>/index.html</welcome-file>
 </welcome-file-list>
</web-app>
